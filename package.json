{
  "name": "multi-project",
  "displayName": "Multi Project",
  "description": "Multi Project",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:multiProjectExplorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "multiProject-explorer",
          "title": "Multi Project Explorer",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "multiProject-explorer": [
        {
          "id": "multiProjectExplorer",
          "name": "Multi Project Explorer",
          "icon": "media/dep.svg",
          "contextualTitle": "Multi Project Explorer"
        }
      ]
    },
    "commands": [
      {
        "command": "multiProjectExplorer.refreshEntry",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "multiProjectExplorer.openFile",
        "title": "Open File"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "multiProjectExplorer.refreshEntry",
          "when": "view == multiProjectExplorer",
          "group": "navigation"
        }
      ],
      "view/item/context": []
    },
    "configuration": [
      {
        "title": "Multi Project",
        "properties": {
          "multiProject.fileName": {
            "type": "string",
            "default": "*",
            "description": "파일의 일부 이름을 포함한 파일을 찾습니다. (*는 모든 파일을 포함합니다.)"
          },
          "multiProject.paths": {
            "type": "array",
            "default": [
              "c:\\Users\\ket87\\OneDrive\\바탕 화면\\TIL",
              "c:\\Users\\ket87\\OneDrive\\바탕 화면\\JS_Design_Pattern"
            ],
            "description": "표현 하고자 하는 프로젝트들의 경로를 설정합니다."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/rimraf": "^3.0.2",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2"
  }
}
